<div class="isp-card">
  <div class="isp-logo">
    {% if logo %}
      <img src="{{ logo }}" alt="{{ name }} logo"/>
    {% else %}
      <span aria-hidden="true">🌐</span>
    {% endif %}
  </div>
  <div>
    <p class="isp-name">{{ name }}</p>
    <p class="isp-speed">{{ avg }}</p>
    <p class="isp-price">{{ price }}</p>
  </div>
</div>
